# ==============================================
# TrailLix Mobile - Production Environment
# Copy this to .env.production and fill in values
# ==============================================

# API Configuration
EXPO_PUBLIC_API_URL=https://api.traillix.com
EXPO_PUBLIC_API_TIMEOUT=30000

# Gemini AI
EXPO_PUBLIC_GEMINI_API_KEY=your-gemini-api-key

# App Configuration
EXPO_PUBLIC_APP_NAME=TrailLix
EXPO_PUBLIC_APP_SCHEME=traillix
EXPO_PUBLIC_ENABLE_DEV_TOOLS=false

# Sentry Configuration
SENTRY_DSN=https://your-sentry-dsn@sentry.io/mobile-project-id
SENTRY_ORG=traillix
SENTRY_PROJECT=traillix-mobile

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true

